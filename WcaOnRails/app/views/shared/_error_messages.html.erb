<% if object.errors.any? %>
  <div id="error_explanation">
    <%= alert :danger do %>
      <%= t 'wca.errors.messages.form_error', count: object.errors.count %>
      <ul>
        <% object.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    <% end %>
  </div>
<% end %>
