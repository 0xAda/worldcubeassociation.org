{
  "name": "wca",
  "description": "Role for wca nodes",
  "json_class": "Chef::Role",
  "default_attributes": {
    "authorization": {
      "sudo": {
        "groups": ["admin", "sudo", "sysadmin"],
        "users": ["vagrant", "cubing"],
        "passwordless": "true"
      }
    }
  },
  "override_attributes": {
  },
  "chef_type": "role",
  "run_list": [
    "recipe[apt]",
    "recipe[sudo]",
    "recipe[wca]",
    "recipe[wca::regulations]",
    "recipe[wca::email]"
  ],
  "env_run_lists": {
  }
}
